<?xml version="1.0"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
  <metadata >
    <id>ucajax</id>
    <version>1.0.4</version>
    <authors>Rick Schott</authors>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>.NET library and jQuery plugin for rendering ASP.NET UserControls via WCF web services.</description>
     <language>en-US</language>
    <projectUrl>https://github.com/rickschott/uc-ajax</projectUrl>
    <licenseUrl>https://github.com/rickschott/uc-ajax/blob/master/license.txt</licenseUrl>
    <tags>ajax asp.net</tags> 
    <dependencies>  
      <dependency id="jQuery"/>
    </dependencies>
  </metadata>
  <files>
     <file src="..\bin\Debug\*.dll" target="lib" />
     <file src="..\Script\*.js" target="Content\Scripts" />
     <file src="..\Service\*.svc" target="Content\Service" />
     <file src="..\Service\web.config" target="Content\Service" />
     <file src="..\Service\web.config.transform" target="Content" />
  </files>
 
</package>